{
  "parserOptions": {
   "ecmaVersion": 6
  },
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "no-multi-spaces": "error",
    "eqeqeq": [
      "error",
      "always"
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "no-console": "off",
    "no-trailing-spaces": "error"
  }
}
